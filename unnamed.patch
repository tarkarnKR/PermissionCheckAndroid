+package com.tarkarn.util;
+
+import java.security.Permission;
+
+import android.Manifest;
+import android.app.Activity;
+import android.content.Context;
+import android.content.pm.PackageManager;
+import android.os.Build;
+import android.support.v4.app.ActivityCompat;
+import android.support.v4.content.ContextCompat;
+import android.util.Log;
+
+/**
+ * PermisionCheck Util.
+ * Over than Android OS Version 6.0.
+ * @author tarkarn
+ * 
+ *    <!-- Dangerous Permission -->
+    	<!-- Phone -->
+	    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
+	    <uses-permission android:name="android.permission.CALL_PHONE" />
+	    <uses-permission android:name="android.permission.READ_CALL_LOG" />
+		<uses-permission android:name="android.permission.WRITE_CALL_LOG" />
+	
+		<!-- Contact -->
+	   	<uses-permission android:name="android.permission.READ_CONTACTS" />
+		<uses-permission android:name="android.permission.WRITE_CONTACTS" />
+	    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
+	    
+	    <!-- SMS -->
+	    <uses-permission android:name="android.permission.SEND_SMS"/>
+	    <uses-permission android:name="android.permission.READ_SMS" />
+	    
+	    <!-- Storage -->
+    	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
+    <!-- Dangerous Permission End -->
+ *
+ */
+public class PermissionCheckUtil {
+
+	private static final String TAG = PermissionCheckUtil.class.getSimpleName();
+	
+	
+	public static void permissionCheck(Activity act){
+
+		// Constant..
+		final int MY_PERMISSIONS_REQUEST_READ_CONTACTS = 2000;
+		
+		// SDK Version..
+		final int SDK_VERSION = Build.VERSION.SDK_INT;
+		Log.d(TAG, " ttTest SDK_VERSION = " + SDK_VERSION);
+		
+		// Check Android Version. (Over than OS 6.0)
+		if(SDK_VERSION >= Build.VERSION_CODES.M){
+			int checkSelfPermission = ContextCompat.checkSelfPermission(act, Manifest.permission.READ_PHONE_STATE);
+			Log.d(TAG, " ttTest checkSelfPermission value = " + checkSelfPermission);
+			
+			if(checkSelfPermission != PackageManager.PERMISSION_GRANTED) {
+				Log.d(TAG, " ttTest checkSelfPermission READ_PHONE_STATE is not PERMISSION_GRANTED ");
+
+				// Should we show an explanation?
+				if(ActivityCompat.shouldShowRequestPermissionRationale(act, Manifest.permission.READ_PHONE_STATE)){
+					Log.d(TAG, " ttTest shouldShowRequestPermissionRationale READ_PHONE_STATE ");
+
+					if(checkSelfPermission != PackageManager.PERMISSION_GRANTED){
+						ActivityCompat.requestPermissions(act, new String[]{Manifest.permission.READ_PHONE_STATE}, MY_PERMISSIONS_REQUEST_READ_CONTACTS);
+					}
+					
+				} else {
+					
+					 // No explanation needed, we can request the permission.
+					Log.d(TAG, " ttTest requestPermissions READ_PHONE_STATE ");
+					ActivityCompat.requestPermissions(act, new String[]{Manifest.permission.READ_PHONE_STATE}, MY_PERMISSIONS_REQUEST_READ_CONTACTS);
+
+				}
+				
+			}
+		}
+		
+	}
+	
+	
+}
